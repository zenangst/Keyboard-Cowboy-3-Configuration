[
  {
    "name" : "GitHub",
    "bundleIdentifier" : "*",
    "bindings" : [
      {
        "enabled" : true,
        "combinations" : [
          "⌃⌥G",
          "G"
        ],
        "name" : "Open Github",
        "commands" : [
          {
            "kind" : "url",
            "enabled" : true,
            "source" : "https://www.github.com",
            "destination": "com.apple.Safari"
          }
        ]
      },
      {
